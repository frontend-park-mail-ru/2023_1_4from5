    {{/if}}


    {{#if logout.showDisplay}}
        <div class="winsettings__chapter">
            <a id="{{logout.id}}" class="winsettings__href" href={{logout.href}}>
                <img class="winsettins__img" src="../../images/logout.svg" alt="logout">
