        {{#if modalWindow.showDisplay}}
            <div class="winsettings__chapter">
                <a id="{{modalWindow.id}}" class="winsettings__href" href={{modalWindow.href}}>
                    <img class="winsettins__img" src="../../images/notification.svg" alt="myPage">
                    <p>{{modalWindow.name}}</p>
                </a>
