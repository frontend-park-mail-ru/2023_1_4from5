{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "no-undef": 0,
    "prefer-const": 0,
    "no-restricted-globals": 0,
    "class-methods-use-this": 0
  }
}
