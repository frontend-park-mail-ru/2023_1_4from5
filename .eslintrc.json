{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "no-restricted-syntax": 0,
    "no-undef": 0,
    "prefer-const": 0,
    "no-restricted-globals": 0,
    "class-methods-use-this": 0,
    "guard-for-in": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "no-case-declarations": 0,
    "no-trailing-spaces": 0,
    "import/no-extraneous-dependencies": 0,
    "object-shorthand": 0,
    "comma-dangle": 0,
    "import/no-cycle": 0,
    "prefer-destructuring": 0,
    "no-await-in-loop": 0,
    "import/no-dynamic-require": 0
  }
}
